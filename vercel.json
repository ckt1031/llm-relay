{
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "api/handler"
		},
		{
			"source": "/robots.txt",
			"destination": "public/robots.txt"
		},
		{
			"source": "/",
			"destination": "public/index.html"
		},
		{
			"source": "/favicon.ico",
			"destination": "public/favicon.ico"
		},
		{
			"source": "/favicon.png",
			"destination": "public/android-chrome-192x192.png"
		}
	]
}
